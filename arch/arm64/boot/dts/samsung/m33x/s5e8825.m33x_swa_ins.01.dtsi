/*
 * pin banks of s5e8825 pin-controller 6 (PERI) : pinctrl@10040000
 * GPP0 ~ GPP3
 * GPG0 ~ GPG2
 * GPB0, GPB1
 */

&pinctrl_6 {
	pinctrl-names = "default";
	pinctrl-0 = <&initial6>;
	initial6: initial-state {
		PIN_IN(gpg0-1, DOWN);	/* BAT_CHECK : NC*/
	};
};


