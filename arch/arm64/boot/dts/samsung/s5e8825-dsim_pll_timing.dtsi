				};

				hs_clk_1124000 {
					pmsk = <
						0x02 0xAD 0x02 0x2C4F
						0x0D 0x04 0x0 0x0
						0x0 0x0 0x0 0x0
						0x0 0x0
						>;
					hs_clk = <1124000>;
				};

				hs_clk_1125000 {
					pmsk = <
						0x02 0xAD 0x02 0x13B1
						0x0D 0x04 0x0 0x0
						0x0 0x0 0x0 0x0
						0x0 0x0
						>;
					hs_clk = <1125000>;
				};

				hs_clk_1100000 {
					pmsk = <
						0x3 0xFE 0x2 0x0 /* p m s k */
						0x0 0x0 0x0 0x0
						0x0 0x0 0x0 0x0
						0x0 0x0
						>;
					hs_clk = <1100000>;
				};

				hs_clk_511000 {
