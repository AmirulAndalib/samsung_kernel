				};

				hs_clk_894000 {
					pmsk = <
						0x2 0x8A 0x1 0x89D9 /* p m s k */
						0x0 0x0 0x0 0x0
						0x0 0x0 0x0 0x0
						0x0 0x0
						>;
					hs_clk = <894000>;
				};
			};
		};
	};
};
