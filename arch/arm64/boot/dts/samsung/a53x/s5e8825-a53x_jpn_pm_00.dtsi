#include "../s5e8825-sec_pm_common.dtsi"

&hsi2c_25 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	s2mpb03_pmic@56 {
		compatible = "samsung,s2mpb03pmic";
		reg = <0x56>;

		s2mpb03,wakeup;

		regulators {
			__ldo1_reg: s2mpb03-ldo1 {
				regulator-name = "VDDD_1.05V_SUB";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1050000>;
			};

			__ldo2_reg: s2mpb03-ldo2 {
				regulator-name = "VDDD_1.1V_CAM";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1100000>;
			};

			__ldo3_reg: s2mpb03-ldo3 {
				regulator-name = "VDDIO_1.8V_CAM";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
			};

			__ldo4_reg: s2mpb03-ldo4 {
				regulator-name = "S2MPB03_LD04";
				regulator-min-microvolt = <1200000>;
				regulator-max-microvolt = <1200000>;
			};

			__ldo5_reg: s2mpb03-ldo5 {
				regulator-name = "VDDA_2.9V_CAM";
				regulator-min-microvolt = <2800000>;
				regulator-max-microvolt = <2900000>;
			};

			__ldo6_reg: s2mpb03-ldo6 {
				regulator-name = "VDDA_2.9V_FCAM";
				regulator-min-microvolt = <2800000>;
				regulator-max-microvolt = <2900000>;
			};

			__ldo7_reg: s2mpb03-ldo7 {
				regulator-name = "VDD_2.8V_AF";
				regulator-min-microvolt = <2800000>;
				regulator-max-microvolt = <2800000>;
			};
		};
	};
};

/ {
	fragment@model {
		target-path = "/";
		__overlay__ {
			sec_thermistor_1: sec_thermistor@1 {
				compatible = "samsung,sec-thermistor";
				status = "okay";
				thermistor_name = "sec-ap-thermistor";
				id = <0>;

				adc_array = <265 315 372 437 519 612 721 846 988 1150
					1332 1512 1741 1970 2203 2434 2667 2874 3067 3238
					3385 3504 3610>;
				temp_array = <900 850 800 750 700 650 600 550 500 450
					  400 350 300 250 200 150 100 50 0 (-50)
					  (-100) (-150) (-200)>;

				io-channels = <&s2mpu13_gpadc 1>;
				io-channel-names = "adc-ap-temp";
				io-channel-ranges;
			};

			sec_thermistor_4: sec_thermistor@4 {
				compatible = "samsung,sec-thermistor";
				status = "okay";
