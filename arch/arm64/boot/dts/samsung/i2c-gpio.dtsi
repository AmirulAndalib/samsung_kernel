&tlmm {
       gpio_i2c_0_sda {
               gpio_i2c_0_sda_default: gpio_i2c_0_sda_default {
                       GPIO_CONFIG_PUD_DRV(AP,tlmm,31, FUNC_INPUT, PULL_NONE, DRV_LV1);
               };
       };
};

&tlmm {
       gpio_i2c_0_scl {
               gpio_i2c_0_scl_default: gpio_i2c_0_scl_default {
                       GPIO_CONFIG_PUD_DRV(AP,tlmm,32, FUNC_INPUT, PULL_NONE, DRV_LV1);
               };
       };
};

&smd {
	/* Software I2C */
	sw_i2c0: i2c@0 {
		cell-index = <0>;
		compatible = "i2c-gpio";
		gpios = <
			SEC_GPIO_REF(AP,tlmm,31) 0 /* SDA */
			SEC_GPIO_REF(AP,tlmm,32) 0 /* SCL */
			>;
		#i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;

		pinctrl-names = "default";
		pinctrl-0 = <
			&gpio_i2c_0_sda_default
			&gpio_i2c_0_scl_default
			>;

		status = "disabled";
	};
};

