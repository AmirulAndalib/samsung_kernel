	};
	charger_thmzone: chg-thmzone {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&chg_thermistor>;
	};
	//-Bug750447,luojianhao.wt,add,20220615,add thermal NTC
};

