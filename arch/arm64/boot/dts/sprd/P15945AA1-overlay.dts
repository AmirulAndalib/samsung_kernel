&reserved_memory {

	logo_reserved: logobuffer@9e000000{
		reg = <0x0 0x9e000000 0x0 0x009e4000>;
	};

#ifdef BSP_KERNEL_CONFIG_WIDEVINE_L1
	drm_reserved: drm-mem@e32ae000 {
		reg = <0x0 0xe32ae000 0x0 0x0b400000>;
	};
#endif

