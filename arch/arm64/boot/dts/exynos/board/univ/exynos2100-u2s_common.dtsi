				ss_tune19 {
					tune_name = "sstx_pre_shoot_ss";
					tune_value = <0x3 0x3>;
				};

				ss_tune20 {
					tune_name = "sstx_amp_ssp";
					tune_value = <0xb 0xb>;
				};

				ss_tune21 {
					tune_name = "sstx_deemp_ssp";
					tune_value = <0x5 0x5>;
				};

				ss_tune22 {
					tune_name = "sstx_pre_shoot_ssp";
					tune_value = <0x3 0x3>;
				};

				ss_tune23 {
					tune_name = "sstx_idrv_up";
					tune_value = <0x7 0x7>;
				};

				ss_tune24 {
					tune_name = "sstx_lfps_idrv_up";
					tune_value = <0x0 0x0>;
				};

				ss_tune25 {
					tune_name = "sstx_up_term";
					tune_value = <0x3 0x3>;
				};

				ss_tune26 {
					tune_name = "sstx_dn_term";
					tune_value = <0x3 0x3>;
				};

				ss_tune27 {
					tune_name = "rext_ovrd";
					tune_value = <0x0 0x0>; /* Use default */
				};
