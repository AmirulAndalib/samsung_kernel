			usb_notifier {
				compatible = "samsung,usb-notifier";
				udc = <&udc>;
				status = "okay";
			};

			acpm_mfd_bus@15960000 {
				status = "okay";
				#address-cells = <1>;
				#size-cells = <0>;
				s2mps23mfd@1f {
					compatible = "samsung,s2mps23mfd";
					acpm-ipc-channel = <2>;
					i2c-speedy-address;
					s2mps23,wakeup = "enabled";
					reg = <0x1f>;
					interrupts = <GIC_SPI INTREQ__COMB_NONSECURE_SYSREG_VGPIO2AP IRQ_TYPE_LEVEL_HIGH>;
					interrupt-parent = <&gic>;
					pinctrl-names = "default";
					pinctrl-0 = <&pm_wrstbi &smpl_warn>;
					gpios = <&gpg0 5 0>;
					/* RTC: wtsr/smpl */
					wtsr_en = "enabled"; /* enable */
					smpl_en = "enabled"; /* enable */
					wtsr_timer_val = <3>;	/* 1000ms */
					smpl_timer_val = <4>;	/* 500ms */
					sub_smpl_en = "enabled"; /* enable */
					check_jigon = <0>; /* do not check jigon */
					/* RTC: If it's first boot, reset rtc to 1/1/2020 12:00:00(Wed) */
					init_time,sec = <0>;
					init_time,min = <0>;
					init_time,hour = <12>;
					init_time,mday = <1>;
					init_time,mon = <0>;
					init_time,year = <120>;
					init_time,wday = <3>;
					/* SMPL_WARN */
					smpl_warn_en = <1>; /* 1 : enable , 0 : disable */
					/* range of SMPL_WARN_LEVEL */
					/* 0x00 : 2.1V, 0x20 : 2.3V, 0x40 : 2.5V, 0x60 : 2.7V 0x80 : 2.9V */
					/* 0xA0 : 3.1V, 0xC0 : 3.3V */
					smpl_warn_vth = <0xA0>;
					/* SMPL_WARN_HYS[4:3] 00(Reserved), 01(200mV), 10(300mV), 11(Reserved) */
					smpl_warn_hys = <0x08>;
					/* OCP_WARN */
					ocp_warn1_en = <0>; /* 1 : enable, 0 : disable */
					ocp_warn1_cnt = <0>; /* ocp warn reset timing control, 0 : 2 clock, 1 : 32 clock */
					ocp_warn1_dvs_mask = <0>; /* ocp warn mask when dvs, 0 : not masked, 1 : masked  */
					ocp_warn1_lv = <0x0>; /* step : -83mA */

					/* Power-meter */
					adc_mode = <1>; /* 0 : disable, 1 : power meter */

					/* CONTROL_SEL# */
					control_sel = <0x64 0x06 0x07 0x77 0x77 0x07 0x70 0x64 0x00 0x00
							0x70 0x88 0x98 0x99 0x09 0x0F 0x00 0x00 0x00 0x00
							0x00>;

					s2mps23_gpadc: s2mps23-gpadc {
						#io-channel-cells = <1>;
						io-channel-ranges;
						acpm-ipc-channel = <14>;
					};

					s2mps23-keys {
						button@1 {
							label = "pmic-keys: KEY_POWER";
							linux,code = <116>;
							wakeup = <1>;
						};
					};

					regulators {
						b1_reg: BUCK1 {
							regulator-name = "vdd_g3d";
							regulator-min-microvolt = <450000>;
							regulator-max-microvolt = <1300000>;
							regulator-always-on;
							regulator-ramp-delay = <6000>;
							regulator-initial-mode = <3>;
						};
