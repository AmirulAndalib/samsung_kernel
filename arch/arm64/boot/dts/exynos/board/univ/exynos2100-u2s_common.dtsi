#define BOARD_ID	0x1
#define BOARD_REV	0x0

/ {
	compatible = "samsung,exynos2100", "samsung,UNIVERSAL2100";
	board_id = <BOARD_ID>;
	board_rev = <BOARD_REV>;

	fragment@model {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;

			model = "Samsung UNIVERSAL2100 board based on EXYNOS2100";

			fixed-rate-clocks {
				oscclk {
					compatible = "samsung,exynos9840-oscclk";
					clock-frequency = <26000000>;
				};
			};

			gpio_keys {
				status = "okay";
				compatible = "gpio-keys";
				pinctrl-names = "default";
				pinctrl-0 = <&key_voldown &key_volup>;

				button@2 {
					label = "gpio-keys: KEY_VOLUMEDOWN";
					linux,code = <114>;
					gpios = <&gpa0 4 0xf>;
				};
				button@3 {
					label = "gpio-keys: KEY_VOLUMEUP";
					linux,code = <115>;
					gpios = <&gpa0 3 0xf>;
				};
			};

			exynos_rgt {
				compatible = "samsung,exynos-rgt";
				num-acpm-pmic = <2>;
			};

			sec-reboot {
				compatible = "samsung,sec-reboot";
				samsung,syscon-phandle = <&pmu_system_controller>;
				reboot-offset = <0x3C00>; /* SYSTEM_CONFIGURATION */
				magic-inform = <0x0808>; /* INFORM2 */
				panic-inform = <0x080C>; /* INFORM3 */
			};

			sec-param {
				compatible = "samsung,sec-param";
				char-table-size = <3>;
				/* 2 columns for character type of param; offset size  */
				char-offset-table = <
					7340592 4    /* CM_OFFSET */
					7340644 4    /* PD_OFFSET */
					7340628 1 >; /* FMM_LOCK_OFFSET */
				u32-table-size = <1>;
				/* 1 column for u32 type of param; offset */
				u32-offset-table = <
					7340632 >;   /* WC_OFFSET */
			};

			usb_hs_tune:hs_tune {
				status = "disabled";
				hs_tune_cnt = <4>;
				/* value = <device host> */
				hs_tune1 {
					tune_name = "tx_pre_emp";
					tune_value = <0x3 0x3>;
				};

				hs_tune2 {
					tune_name = "tx_vref";
					tune_value = <0xc 0xc>;
				};

				hs_tune3 {
					tune_name = "rx_sqrx";
					tune_value = <0x5 0x5>;
				};

				hs_tune4 {
					tune_name = "utmi_clk";
					tune_value = <0x1 0x1>;
				};
			};

			usb_ss_tune:ss_tune {
				status = "disabled";
				ss_tune_cnt = <27>;
				/* value = <device host> */
				ss_tune1 {
					tune_name = "ssrx_sqhs_th_ssp";
					tune_value = <0x4 0x4>;
				};

				ss_tune2 {
					tune_name = "ssrx_lfps_th";
					tune_value = <0x2 0x2>;
				};

				ss_tune3 {
					tune_name = "ssrx_adap_coef_sel";
					tune_value = <0x1 0x1>;
				};

				ss_tune4 {
					tune_name = "ssrx_mf_eq_ctrl_ss";
					tune_value = <0x6 0x6>;
				};

				ss_tune5 {
					tune_name = "ssrx_mf_eq_ctrl_ssp";
					tune_value = <0x6 0x6>;
				};

				ss_tune6 {
					tune_name = "ssrx_hf_eq_ctrl_ss";
					tune_value = <0xe 0xe>;
				};

				ss_tune7 {
					tune_name = "ssrx_hf_eq_ctrl_ssp";
					tune_value = <0xe 0xe>;
				};

				ss_tune8 {
					tune_name = "ssrx_dfe_1tap_ctrl_ss";
					tune_value = <0x2 0x2>;
				};

				ss_tune9 {
					tune_name = "ssrx_dfe_1tap_ctrl_ssp";
					tune_value = <0x2 0x2>;
				};

				ss_tune10 {
					tune_name = "ssrx_dfe_2tap_ctrl_ss";
