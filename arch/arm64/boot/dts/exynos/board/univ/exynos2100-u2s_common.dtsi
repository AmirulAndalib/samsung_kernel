				};

				ss_tune16 {
					tune_name = "ssrx_term_cal";
					tune_value = <0xb 0xb>;
				};

				ss_tune17 {
					tune_name = "sstx_amp_ss";
					tune_value = <0xb 0xb>;
				};

				ss_tune18 {
					tune_name = "sstx_deemp_ss";
					tune_value = <0x5 0x5>;
