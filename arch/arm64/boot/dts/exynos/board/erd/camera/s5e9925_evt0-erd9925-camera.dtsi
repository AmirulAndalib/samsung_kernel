#include <dt-bindings/camera/exynos_is_dt.h>

#include "module_evt0_2ld.dtsi" /* REAR CAMERA */
#include "module_3h1.dtsi" /* FRONT CAMERA */
#include "module_2la.dtsi" /* Ultra Wide CAMERA */
#include "module_evt0_gw2.dtsi" /* Tele CAMERA */

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			is_flash_s2mu106: is-flash-s2mu106@0 {
				compatible = "samsung,sensor-flash-s2mu106";
				sensor-module = <&is_sensor_2ld>;
				id = <0>;
				led_ch = <1 2>;
				status = "okay";
			};

			is_flash_s2mu106_2: is-flash-s2mu106_2@0 {
				compatible = "samsung,sensor-flash-s2mu106";
				sensor-module = <&is_sensor_gw2>;
				id = <2>;
				led_ch = <1 2>;
				status = "okay";
			};

			is_flash_s2mu106_4: is-flash-s2mu106_4@0 {
				compatible = "samsung,sensor-flash-s2mu106";
				sensor-module = <&is_sensor_2la>;
				id = <4>;
				led_ch = <1 2>;
				status = "okay";
			};

			/* REAR CAMERA */
			is_sensor0: is_sensor0@17030000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17030000 0x1000>, /* MIPI-CSI0 */
					<0x0 0x173E0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
				scenario = <SENSOR_SCENARIO_NORMAL>;	/* Normal, Vision, OIS etc */
				id = <0>;
				csi_ch = <0>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS0 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis0_m0s4s4s4s4s4s4_s22 0>;
			};

			/* FRONT CAMERA */
			is_sensor1: is_sensor1@17040000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17080000 0x1000>, /* MIPI-CSI5 */
					<0x0 0x173E0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <5>;
				status = "okay";

				interrupts = <0 INTREQ__CSIS5 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis5_m0s4s4s4s4s4s4_s22 0>;
			};

			/* Tele CAMERA */
			is_sensor2: is_sensor2@17050000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17040000 0x1000>, /* MIPI-CSI1 */
					<0x0 0x173E0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <2>;
				csi_ch = <1>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis1_m0s4s4s4s4s4s4_s22 0>;
			};

			is_sensor3: is_sensor3@17060000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <3>;
				csi_ch = <4>;
				status = "disabled";
			};

			/* ULTRA WIDE CAMERA */
			is_sensor4: is_sensor4@17070000 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0x0 0x17050000 0x1000>, /* MIPI-CSI2 */
					<0x0 0x173E0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <4>;
				csi_ch = <2>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS2 IRQ_TYPE_LEVEL_HIGH>;
				phys = <&mipi_phy_csis2_m0s4s4s4s4s4s4_s22 0>;
			};

			is_sensor5: is_sensor5@17080000 {
				#address-cells = <1>;
				#size-cells = <1>;
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <1>;
				status = "disabled";
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/
&pinctrl_1 {
	/* REAR TELE */
	hsi2c25_bus_in: hsi2c25-bus-in {
		samsung,pins = "gpm1-0", "gpm1-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR WIDE/TELE AF */
	hsi2c27_bus_in: hsi2c27-bus-in {
		samsung,pins = "gpm3-0", "gpm3-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR WIDE */
	hsi2c24_bus_in: hsi2c24-bus-in {
		samsung,pins = "gpm0-0", "gpm0-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* U WIDE */
	hsi2c26_bus_in: hsi2c26-bus-in {
		samsung,pins = "gpm2-0", "gpm2-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Front */
	hsi2c32_bus_in: hsi2c32-bus-in {
		samsung,pins = "gpm8-0", "gpm8-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&pinctrl_5 {
	/* FRONT AF*/
	hsi2c14_bus_in: hsi2c14-bus-in {
		samsung,pins = "gpp7-0", "gpp7-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&hsi2c_32 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpm8 0 0 &gpm8 1 0>;
	status = "okay";
	clock-frequency = <400000>;
	samsung,reset-before-trans;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c32_bus_in>;
	pinctrl-1 = <&hsi2c32_bus>;
	pinctrl-2 = <&hsi2c32_bus_in>;

	is-3h1@35 {
		compatible = "samsung,exynos-is-cis-3h1";
		sensor-module = <&is_sensor_3h1>;
		reg = <0x35>; /* 1 bit right shift */
		id = <1>; /* matching is_sensor id */
		setfile = "default";
	};
};

&hsi2c_24 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpm0 0 0 &gpm0 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c24_bus_in>;
	pinctrl-1 = <&hsi2c24_bus>;
	pinctrl-2 = <&hsi2c24_bus_in>;

	is-2ld@20 {
		compatible = "samsung,exynos-is-cis-2ld";
		sensor-module = <&is_sensor_2ld>;
		reg = <0x10>; /* 1 bit right shift */
		id = <0>; /* matching is_sensor id */
		setfile = "default";
		rev_reg = <0x0002 2 0xA301>;
	};
};

&hsi2c_25 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpm1 0 0 &gpm1 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

