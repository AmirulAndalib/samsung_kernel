		need_smc = <0x10210204>;
		cmu_id = <0x10200000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_npu0: pd-npu0@11862b80 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380001>;
		need_smc = <0x10a10204>;
		cmu_id = <0x10a00000>;
		status = "okay";
		parent = <&pd_npus>;
		#power-domain-cells = <0>;
	};

	pd_csis: pd-csis@11862680 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380006>;
		need_smc = <0x15010204>;
		cmu_id = <0x15000000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_isp: pd-isp@11862900 {
