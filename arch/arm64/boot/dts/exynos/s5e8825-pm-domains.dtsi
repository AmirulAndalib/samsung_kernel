#include <dt-bindings/power/exynos-power.h>

/ {
	pd_aud: pd-aud@11862080 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380000>;
		need_smc = <0x14C10204>;
		cmu_id = <0x14e00000>;
		status = "okay";
		power-down-ok = <PD_OK_AUD>;
		#power-domain-cells = <0>;
		skip-idle-ip;
	};

	pd_npus: pd-npus@11862c00 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380002>;
		need_smc = <0x10B10204>;
		cmu_id = <0x10B00000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_dpu: pd-dpu@11862700 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380003>;
		need_smc = <0x14810204>;
		cmu_id = <0x14800000>;
		status = "okay";
		#power-domain-cells = <0>;
		pd-boot-on;
		skip-idle-ip;
	};

	pd_m2m: pd-m2m@11862780 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380004>;
		need_smc = <0x12c10204>;
		cmu_id = <0x12c00000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_g3d: pd-g3d@11862820 {
		compatible = "samsung,exynos-pd";
