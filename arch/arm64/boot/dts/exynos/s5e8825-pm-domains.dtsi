		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_mcsc: pd-mcsc@11862980 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380008>;
		need_smc = <0x15610204>;
		cmu_id = <0x15600000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_mfc: pd-mfc@11862a00 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB1380009>;
		need_smc = < 0x12E10204>;
		cmu_id = <0x12E00000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_vts: pd-vts@11862280 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB138000A>;
		need_smc = <0x11610204>;
		cmu_id = <0x11700000>;
		status = "okay";
		power-down-ok = <PD_OK_VTS>;
		#power-domain-cells = <0>;
		skip-idle-ip;
	};

	pd_usb: pd-usb@11862e00 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB138000B>;
		need_smc = <0x13010204>;
		cmu_id = <0x13000000>;
		status = "okay";
		#power-domain-cells = <0>;
		power-down-ok = <PD_OK_USB>;
		skip-idle-ip;
	};

	pd_tnr: pd-tnr@11862d80 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB138000C>;
		need_smc = <0x15310204>;
		cmu_id = <0x15300000>;
		status = "okay";
		#power-domain-cells = <0>;
	};

	pd_taa: pd-taa@11862d00 {
		compatible = "samsung,exynos-pd";
		cal_id = <0xB138000D>;
		need_smc = <0x15510204>;
		cmu_id = <0x15500000>;
		status = "okay";
		parent = <&pd_csis>;
		#power-domain-cells = <0>;
	};

	dbgdev-pd-aud {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_aud>;
	};

	dbgdev-pd-npu0 {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_npu0>;
	};

	dbgdev-pd-npus {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_npus>;
	};

	dbgdev-pd-dpu {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_dpu>;
	};

	dbgdev-pd-m2m {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_m2m>;
	};

	dbgdev-pd-g3d {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_g3d>;
	};

	dbgdev-pd-csis {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_csis>;
	};

	dbgdev-pd-isp {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_isp>;
	};

	dbgdev-pd-mcsc {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_mcsc>;
	};

	dbgdev-pd-mfc {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_mfc>;
	};

	dbgdev-pd-vts {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_vts>;
	};

	dbgdev-pd-usb {
		compatible = "samsung,exynos-pd-dbg";
		power-domains = <&pd_usb>;
	};

	dbgdev-pd-tnr {
