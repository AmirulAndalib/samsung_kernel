/ {
	drmdpp0: drmdpp@0x19CB0000 {	/* L0 */
		compatible = "samsung,exynos-dpp";
		reg = <0x0 0x19CB0000 0x1000>,	/* DPU_DMA */
		      <0x0 0x19CD0000 0x1000>,	/* DPP */
		      <0x0 0x19CA0000 0x500>;	/* vOTF_DPUF0 */
		reg-names = "dma", "dpp", "votf";

		/* DPU_DMA IRQ, DPP IRQ */
		interrupts = <GIC_SPI INTREQ__DPUF0_DMA_L0 IRQ_TYPE_LEVEL_HIGH>,
			   <GIC_SPI INTREQ__DPUF0_DPP_L0 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "dma", "dpp";

		/* Each bit indicates DPP attribute */
		/* 0:AFBC 1:BLOCK 2:FLIP 3:ROT 4:CSC 5:SCALE 6:HDR 7:C_HDR */
		/* 8:C_HDR10 9:WCG 10:SBWC 16:IDMA 17:ODMA 18:DPP */
		attr = <0x50004>;
		port = <0>; /* AXI port number */

		/* 1: scale X, 2: 1/2x scale down, 4: 1/4x scale down */
		scale_down = <1>;
		/* 1: scale X, 2: 2x scale up, 4: 4x scale up */
		scale_up = <1>;

		dpp,id = <0>;
	};

	drmdpp1: drmdpp@0x19CB1000 {	/* L1 */
		compatible = "samsung,exynos-dpp";
		reg = <0x0 0x19CB1000 0x1000>, <0x0 0x19CD1000 0x1000>;
		reg-names = "dma", "dpp";

		interrupts = <GIC_SPI INTREQ__DPUF0_DMA_L1 IRQ_TYPE_LEVEL_HIGH>,
			   <GIC_SPI INTREQ__DPUF0_DPP_L1 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "dma", "dpp";

		attr = <0x5043D>;
		port = <0>;
		scale_down = <4>;
		scale_up = <8>;

		dpp,id = <1>;
		dpp,video;
	};

	drmdpp2: drmdpp@0x19CB2000 {	/* L2 */
		compatible = "samsung,exynos-dpp";
		reg = <0x0 0x19CB2000 0x1000>, <0x0 0x19CD2000 0x1000>;
		reg-names = "dma", "dpp";

		interrupts = <GIC_SPI INTREQ__DPUF0_DMA_L2 IRQ_TYPE_LEVEL_HIGH>,
			   <GIC_SPI INTREQ__DPUF0_DPP_L2 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "dma", "dpp";

		attr = <0x50005>;
		port = <0>;
		scale_down = <1>;
		scale_up = <1>;

		dpp,id = <2>;
	};

	drmdpp3: drmdpp@0x19CB3000 {	/* L3 */
