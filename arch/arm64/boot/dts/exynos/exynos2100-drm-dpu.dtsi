		interrupt-names = "dma", "dpp";

		attr = <0x50434>;
		port = <0>;
		scale_down = <4>;
		scale_up = <8>;

		dpp,id = <3>;
		dpp,video;
	};

	drmdpp4: drmdpp@0x19CB4000 {	/* L4 */
